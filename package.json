{
  "name": "time-left",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest src",
    "coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/svelte": "^3.1.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^28.1.3",
    "happy-dom": "^5.3.4",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jsdom": "^20.0.0",
    "sass": "^1.53.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-jester": "^2.3.2",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^28.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "svelte",
      "ts"
    ],
    "testEnvironment": "jsdom"
  },
  "setupFilesAfterEnv": [
    "@testing-library/jest-dom/extend-expect"
  ]
}
